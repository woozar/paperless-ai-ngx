{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalEnv": [
    "ADMIN_INITIAL_PASSWORD",
    "API_TOKEN",
    "DATABASE_URL",
    "ENCRYPTION_KEY",
    "HOST",
    "JWT_SECRET",
    "LOG_LEVEL",
    "NEXT_RUNTIME",
    "NODE_ENV",
    "PORT",
    "WEBAUTHN_RP_ID",
    "WEBAUTHN_ORIGIN"
  ],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:coverage": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false
    },
    "db:migrate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    }
  }
}
